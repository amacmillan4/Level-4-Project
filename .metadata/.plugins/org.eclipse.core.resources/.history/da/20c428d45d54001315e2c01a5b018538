

public class a {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Method method = new Method("Aberafan", "&-36-14-58-16-34-58-56-58,12", 100, 8);
		
		int i = 0;
		while (i < 100){
			System.out.print(method.calcNext());
			i++;
			
			if (i % 8 == 0){
				System.out.println();
			}
			
			if (i > 46){
				method.swapRounds();
			}
		}
	}

}
